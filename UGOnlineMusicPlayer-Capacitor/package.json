{
  "name": "ugonlinemusicplayer-capacitor",
  "version": "1.0.2",
  "description": "UG在线音乐播放器的Android应用版本",
  "main": "index.js",
  "scripts": {
    "start": "npx serve www -l 8100",
    "add-android": "npx cap add android",
