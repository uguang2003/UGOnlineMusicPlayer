    "build-release": "cd android && gradlew.bat assembleRelease"
  },
  "keywords": [
    "music",
    "player",
    "capacitor",
    "android"
  ],
  "author": "UG",
  "license": "ISC",
  "dependencies": {
    "@capacitor-community/media": "^5.0.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/local-notifications": "^5.0.0",
    "@capacitor/network": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "serve": "^14.2.1"
  }
}